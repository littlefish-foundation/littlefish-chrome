{
    "name": "littlefish Foundation",
    "description": "Everything you imagine is real.",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "bookmarks"
      ],
    "icons": {
        "16": "icons/icon_16.png",
        "32": "icons/icon_32.png",
        "48": "icons/icon_48.png",
        "128": "icons/icon_128.png"
      },
      "background": {
        "service_worker": "background.js"
      },
      "chrome_url_overrides": {
        "newtab": "index.html"
      }
  }
